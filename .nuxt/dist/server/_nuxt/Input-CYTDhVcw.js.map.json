{"file":"Input-CYTDhVcw.js","mappings":";;;;;;;;;;;;;;;;;;;;;;AAkBC,UAAM,QAAQA,8BAAY;AAE1B,UAAM,QAAQ;AAad,UAAM,UAAU,SAAS,MAAM,MAAM,MAAM,SAAS,KAAK,OAAM,EAAG,SAAS,EAAE,EAAE,OAAO,GAAG,CAAC,CAAC,EAAE;AAE7F,UAAM,sBAAsB,SAAS,MAAM;AAC1C,aAAO,MAAM,eAAe,MAAM,SAAS;AAAA,IAC7C,CAAE;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;","names":["_useModel"],"sources":["../../../../components/FormGroup/Input.vue"],"sourcesContent":["<template>\n\t<div class=\"form-group\" :class=\"col\">\n\t\t<label v-if=\"label\" :for=\"inputId\" class=\"label\">\n\t\t\t{{ label }}:\n\t\t\t<code v-if=\"message\">{{ message }}</code>\n\t\t</label>\n\t\t<div class=\"input bg-border-input d-flex align-items-center\">\n\t\t\t<input :id=\"inputId\" :type=\"type\" v-model=\"model\" class=\"form-control\" :class=\"{ 'is-invalid': error }\" :placeholder=\"computedPlaceholder\" :step=\"step\" :required=\"required\" :readonly=\"readonly\" />\n\t\t</div>\n\t\t<div v-if=\"error\" class=\"invalid-feedback d-block\">\n\t\t\t{{ error }}\n\t\t</div>\n\t</div>\n</template>\n\n<script setup>\n\timport { computed } from 'vue'\n\n\tconst model = defineModel();\n\n\tconst props = defineProps({\n\t\tlabel: String,\n\t\ttype: { type: String, default: 'text' },\n\t\tstep: { type: String, default: null },\n\t\tcol: { type: String, default: 'col-md-12' },\n\t\tid: String,\n\t\tplaceholder: String,\n\t\trequired: Boolean,\n\t\treadonly: { type: Boolean, default: false },\n\t\terror: String,\n\t\tmessage: String\n\t});\n\n\tconst inputId = computed(() => props.id || `input-${Math.random().toString(36).substr(2, 9)}`)\n\n\tconst computedPlaceholder = computed(() => {\n\t\treturn props.placeholder || props.label || '';\n\t});\n\n</script>\n"],"version":3}